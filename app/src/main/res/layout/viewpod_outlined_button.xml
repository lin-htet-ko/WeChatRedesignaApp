<?xml version="1.0" encoding="utf-8"?>
<com.linhteko.wechat.viewpods.OutlinedButtonViewPod xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/background_outlined_button"
    android:paddingHorizontal="@dimen/padding_horizontal_btn"
    android:paddingVertical="@dimen/padding_verticle_btn">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvOutlinedBtn"
        style="@style/text_style_for_custom_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="?attr/colorPrimary"
        tools:text="@string/app_name" />

</com.linhteko.wechat.viewpods.OutlinedButtonViewPod>